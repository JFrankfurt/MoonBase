{
  "name": "to-the-moon",
  "license": "MIT",
  "engines": {
    "node": "^10.0.0",
    "yarn": "^1.9.0"
  },
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@types/node": "^10.12.0",
    "@zeit/next-typescript": "^1.1.1",
    "express": "^4.16.4",
    "helmet": "^3.14.0",
    "kjua": "^0.1.2",
    "next": "^7.0.2",
    "phaser": "^3.15.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-jss": "^8.6.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/helmet": "^0.0.42",
    "@types/next": "^7.0.3",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-jss": "^8.6.0",
    "@types/styled-jsx": "^2.2.7",
    "ts-node": "^7.0.1"
  },
  "scripts": {
    "dev": "nodemon server/app.ts",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production node production-server/server/app.js",
    "now": "now && now alias",
    "now-rm": "now rm to-the-moon --safe --yes",
    "lint": "tslint -p tsconfig.json && tslint -p tsconfig.server.json",
    "processes-3000": "ss -lptn 'sport = :3000'",
    "processes-node": "ps -e|grep node",
    "kill-processes-at-port": "lsof -i tcp:3000 | awk 'NR!=1 {print $2}' | xargs kill"
  }
}
